{
	"name": "GitHub userscript",
	"version": "0.1.0",
	"manifest_version": 2,

	"description": "日時表示を絶対時刻化・ソースコードを<code>で括って翻訳されないようにする",

	"content_scripts": [{
		"matches": [
			"https://github.com/*"
		],
		"js": [
			"js/translate-only-comment.js",
			"js/pre2code.js",
			"js/change_datetime.js"
		]
	}]
}
